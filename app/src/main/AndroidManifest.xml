<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healer.englishpronounciation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:largeHeap="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StudyMainActivity"
            android:label="@string/title_activity_study_main"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName="com.example.healer.englishpronounciation.MainActivity"
            ></activity>
        <service android:name=".service.MyService"></service>
    </application>

</manifest>